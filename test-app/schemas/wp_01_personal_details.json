              "id": "australian_resident",
              "label": "Are you an Australian Resident?",
              "field_info": {
                "field_type": "boolean",
                "required": true
              }
            },
            {
              "id": "residency_start_date",
              "label": "Australian residency start date",
              "field_info": {
                "field_type": "date"
              }
