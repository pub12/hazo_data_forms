      {
        "sub_section_id": "name_details",
        "sub_section_label": "Name Details",
        "field_group": {
          "orientation": "horizontal",
          "fields": [
            {
              "id": "title",
              "label": "Title",
              "field_info": {
                "field_type": "text",
                "placeholder": "Mr, Ms, Dr, etc."
              }
            },
            {
              "id": "given_name",
              "label": "Given Name",
              "field_info": {
                "field_type": "text",
                "required": true
              }
            },
            {
              "id": "family_name",
              "label": "Family Name",
              "field_info": {
                "field_type": "text",
                "required": true
              }
            },
            {
              "id": "other_name",
              "label": "Other Name",
              "field_info": {
                "field_type": "text"
              }
